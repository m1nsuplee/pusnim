generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        Int     @id @default(autoincrement())
  title     String
  completed Boolean @default(false)
  priority Priority
}

model Person {
  id   Int    @id @default(autoincrement())
  name String
  height Int
}

enum Priority {
  HIGH
  MEDIUM
  LOW
}
